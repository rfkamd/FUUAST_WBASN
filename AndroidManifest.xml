<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fuuast_wbasn"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="10" />

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.fuuast_wbasn.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.fuuast_wbasn.Activity2"
             android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" 
            android:label="@string/title_activity_activity2" >
        </activity>

        <service
            android:name="com.example.fuuast_wbasn.MyService"
            android:permission="android.permission.BLUETOOTH_ADMIN" >
        </service>

        <activity
            android:name="com.example.fuuast_wbasn.listActivity"
             android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" 
            android:label="@string/title_activity_view_sensor" >
        </activity>
        <activity
            android:name="com.example.fuuast_wbasn.BPlist"
             android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" 
            android:label="@string/title_activity_bplist" >
        </activity>
        <activity
            android:name="com.example.fuuast_wbasn.ChartActivity"
            android:label="@string/title_activity_chart"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name="com.example.fuuast_wbasn.TempChartActivity"
            android:label="@string/title_activity_temp_chart"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name="com.example.fuuast_wbasn.Settings"
             android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" 
            android:label="@string/title_activity_settings" >
        </activity>
    </application>

</manifest>